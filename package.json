{
  "name": "wsl-manager",
  "version": "1.0.0",
  "description": "This app will be used to manage WSL linux distribution on winodws 10 or higer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "start-force": "npm install && electron ./app",
