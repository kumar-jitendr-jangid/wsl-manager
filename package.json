    "wsl-manager",
    "wsl",
    "backup",
    "wsl",
    "linux",
    "manager"
  ],
  "author": "jitendra kumar jangid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kumar-jitendr-jangid/wsl-manager/issues"
  },
  "homepage": "https://github.com/kumar-jitendr-jangid/wsl-manager#readme",
  "dependencies": {
    "electron-store": "^8.1.0",
    "node-cmd": "^5.0.0",
    "path": "^0.12.7",
    "powershell": "^2.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "electron": "^24.2.0",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "build": {
    "appId": "pnsj-wsl-001",
